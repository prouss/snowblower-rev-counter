<OPT QUIET IMGXY NODEPEND>

<FILE TEMPLATE="htp.htt">

<DEF NAME="SUBPROJECT" OPTION="text href">
  <li class="pseudolink" onclick="location='./$href'"><USE text></li>
</DEF>

<DEF NAME="LOG-ITEM" OPTION="date text href">
  <hr style="margin: 0; margin-bottom: 0.5rem;">
  <div class="pseudolink" onclick="location='./$href'"><p><USE date></p><p class="log-item"><USE text></p></div>
</DEF>

<DEF NAME="IMAGE" OPTION="src alt">
	<a href="images/$src"><img src="images/$src" alt="$alt"></a>
</DEF>

<DEF NAME="SMALL_IMAGE" OPTION="src alt">
	<a href="images/$src"><img src="images/small $src" alt="$alt"></a>
</DEF>
